{
  "name": "cigna",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "ng serve --proxy-config proxy.conf.js",
    "begin": "node server.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env2json npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/flex-layout": "git+https://github.com/angular/flex-layout-builds.git",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngx-translate/core": "^8.0.0",
    "@swimlane/ngx-dnd": "^3.0.0",
    "@swimlane/ngx-ui": "^18.2.0",
    "angular2-moment": "^1.7.0",
    "bootstrap": "4.0.0-beta",
    "bootswatch": "^3.3.7",
    "codemirror": "^5.31.0",
    "core-js": "^2.4.1",
    "cuppa-ng2-slidemenu": "^1.0.2",
    "express": "^4.16.2",
    "express-locale": "^1.0.4",
    "express-ntlm": "^2.2.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongoose": "^4.12.6",
    "mongoose-deep-populate": "^3.0.0",
    "msinghthakur": "^2.2.0",
    "ng2-file-upload": "^1.2.1",
    "ngx-restangular": "^1.0.13",
    "nodemailer": "^4.3.1",
    "passport": "^0.4.0",
    "popper.js": "^1.12.6",
    "rxjs": "^5.4.0",
    "socket.io": "^2.0.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.2",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^1.0.4",
    "@types/jasmine": "^2.5.52",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.52",
    "@types/node": "^8.0.10",
    "codelyzer": "~3.2.0",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.1.2",
    "puppeteer": "^0.11.0",
    "stylelint": "~8.1.0",
    "stylelint-config-recommended-scss": "~2.0.0",
    "stylelint-config-standard": "~17.0.0",
    "stylelint-scss": "~2.1.0",
    "ts-node": "^3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
